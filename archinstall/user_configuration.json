	"custom-commands": [
		"mkdir -p /tmp/yay-install",
		"sudo -H -u me curl -o \"yay.tar.gz\" \"https://aur.archlinux.org/cgit/aur.git/snapshot/yay.tar.gz\"",
		"sudo -H -u me tar -zxf \"yay.tar.gz\"",
		"rm \"yay.tar.gz\"",
		"cd yay",
		"su me -c \"makepkg -csi --noconfirm\"",
		"[[ -d \"/tmp/yay-install\" ]] && rm -rf \"/tmp/yay-install\"",
		"yay install nvidia-470xx-dkms"
	]
