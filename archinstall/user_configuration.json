	"script": "guided",
	"silent": false,
	"sys-language": "en_GB.UTF-8",
	"timezone": "Europe/London",
	"version": "2.5.0",
	"custom-commands": [
		"echo \"Configuring sudo and user (archinstall isn't doing this for some reason)\"",
		"groupadd sudo",
		"sudo -H -u me xdg-user-dirs-update",
		"usermod -aG sudo,wheel,users -s /bin/zsh me",
		"echo \"Installing yay\"",
		"git clone https://aur.archlinux.org/yay.git ./yay",
		"ls -al ./yay",
		"cd ./yay",
		"sudo -H -u me makepkg -si --noconfirm",
		"cd .. && rm -rf ./yay",
		"echo \"Installing AUR packages\"",
		"sudo -H -u me yay install -S --needed --noconfirm nvidia-470xx-dkms nvidia-470xx-settings nvidia-470xx-utils swtichboard-plug-pantheon-tweaks-git"
	]
