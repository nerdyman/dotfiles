{
  "apollographql.display.showRunInStudioButton": false,
  "files.trimTrailingWhitespace": true,
  "svg.completion.showAdvanced": true,
  "workbench.editor.empty.hint": "hidden",
  "search.mode": "newEditor",
  "debug.inlineValues": "on",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.copyWithSyntaxHighlighting": false,
  "editor.find.seedSearchStringFromSelection": "never",
  "editor.fontFamily": "Hasklig",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.fontWeight": "500",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.letterSpacing": 0.1,
  "editor.lineHeight": 32,
  "editor.multiDocumentOccurrencesHighlight": true,
  "editor.multiCursorModifier": "alt",
  "editor.renderWhitespace": "boundary",
  "editor.roundedSelection": true,
  "editor.rulers": [120],
  "editor.smoothScrolling": true,
  "editor.tabCompletion": "on",
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.decorations.colors": true,
  "files.associations": {
    "*.ejs": "html"
  },
  "files.enableTrash": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/tmp": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    ".env": false,
    ".env.*": false
  },
  "files.insertFinalNewline": true,
  "git.decorations.enabled": true,
  "javascript.suggest.autoImports": false,
  "markdown.links.openLocation": "beside",
  "markdown.preview.fontFamily": "system-ui",
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "*.ignore": true,
    ".env": false
  },
  "search.quickOpen.includeHistory": false,
  "search.showLineNumbers": true,
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.external.linuxExec": "kitty",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.fontFamily": "Hasklig",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.fontWeightBold": "600",
  "typescript.suggest.autoImports": false,
  "chat.editor.lineHeight": 1.5,
  "chat.editor.fontSize": 13,
  "window.restoreWindows": "all",
  "window.zoomLevel": 0.25,
  "workbench.colorCustomizations": {
    "[Cryptic]": {
      "editor.lineHighlightBackground": "#eb83f711",
      "editor.lineHighlightBorder": "#ff000000",
      "editorIndentGuide.activeBackground1": "#f783ac66",
      "editorIndentGuide.background1": "#f783ac22"
    },
    "editor.selectionBackground": "#ffffff44",
    "editor.selectionHighlightBorder": "#ffffff80",
    "editorCursor.foreground": "#f783ac",
    "editorError.background": "#dd00004a",
    "editorWarning.background": "#ffd0004f",
    "terminalCursor.foreground": "#f783ac"
  },
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.iconTheme": "eq-material-theme-icons-palenight",
  "workbench.list.horizontalScrolling": true,
  "workbench.settings.useSplitJSON": false,
  "workbench.startupEditor": "welcomePage",
  "workbench.tree.indent": 12,
  "gitlens.codeLens.enabled": false,
  "terminal.external.osxExec": "Kitty.app",
  "editor.stickyScroll.enabled": true,
  "prettier.requireConfig": true,
  "workbench.colorTheme": "Cryptic",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "markdown.preview.scrollPreviewWithEditor": false,
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": true,
  "totalTypeScript.hiddenTips": [
    "passing-generics-to-types",
    "record-utility-type",
    "as-const",
    "as-const-on-object",
    "typeof",
    "pick-utility-type",
    "omit-utility-type",
    "type-alias-with-generics",
    "interface-with-generics",
    "interface-with-multiple-generics",
    "type-alias-with-multiple-generics",
    "keyof",
    "mapped-type",
    "required-utility-type",
    "partial-utility-type",
    "generic-slots-in-functions",
    "returntype-utility-type",
    "tuple-type",
    "non-null-expression",
    "uppercase-utility-type",
    "in-operator-narrowing"
  ],
  "search.searchEditor.defaultNumberOfContextLines": 5,
  "markdown.preview.fontSize": 18,
  "git.openRepositoryInParentFolders": "never",
  "tailwindCSS.experimental.classRegex": [
    [
      "cva\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ],
    [
      "cx\\(([^)]*)\\)",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ],
  ],
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "aws.telemetry": false,
  "shellcheck.ignorePatterns": {
    "**/.env": true,
    "**/.env.*": true
  },

  /**
   * Languages.
   */
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  /**
   * Ruby.
   */
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.semanticHighlighting.enabled": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [
      120
    ],
    "editor.defaultFormatter": "LoranKloeze.ruby-rubocop-revived"
  },
  "solargraph.commandPath": "/Users/me/.rbenv/shims/solargraph",
  "rubyLsp.rubyVersionManager": "rbenv",
  "ruby.rubocop.executePath": "/Users/me/.rbenv/shims/",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.sideBar.location": "right",
}
