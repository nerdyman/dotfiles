{
  "editor.bracketPairColorization.enabled": true,
  "editor.matchBrackets": "near",
  "editor.fontFamily": "Hasklig",
  "editor.fontWeight": "600",
  "editor.renderWhitespace": "boundary",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.letterSpacing": 0.1,
  "editor.lineHeight": 32,
  "editor.smoothScrolling": true,
  "editor.rulers": [100],
  "editor.tabCompletion": "on",
  "editor.roundedSelection": true,
  "workbench.list.horizontalScrolling": true,
  "workbench.startupEditor": "welcomePage",
  "workbench.tree.indent": 16,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.multiCursorModifier": "alt",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "workbench.settings.useSplitJSON": false,
  "window.restoreWindows": "all",
  "search.exclude": {
    "*.ignore": true
  },
  "search.showLineNumbers": true,
  "debug.inlineValues": true,
  "terminal.explorerKind": "external",
  "terminal.external.linuxExec": "kitty",
  "telemetry.telemetryLevel": "off",
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "html.format.endWithNewline": true,
  "markdown.preview.fontFamily": "system-ui",
  "prettier.requireConfig": true,
  "workbench.colorCustomizations": {
    "editorError.background": "#dd00004a",
    "editorWarning.background": "#ffd0004f",
    "editor.selectionBackground": "#ffffff44",
    "editor.selectionHighlightBorder": "#ffffff80",
    "editorCursor.foreground": "#f783ac",
    "terminalCursor.foreground": "#f783ac",
    "[Cryptic]": {
      "editorIndentGuide.activeBackground": "#f783ac66",
      "editor.lineHighlightBackground": "#eb83f711",
      "editorIndentGuide.background": "#f783ac22",
      "editor.lineHighlightBorder": "#ff000000"
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.trailingComma": "es5",
  "git.decorations.enabled": true,
  "explorer.decorations.colors": true,
  "gitlens.statusBar.alignment": "left",
  "editor.fontLigatures": true,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "search.quickOpen.includeHistory": false,
  "javascript.suggest.autoImports": false,
  "typescript.suggest.autoImports": false,
  "editor.find.seedSearchStringFromSelection": "never",
  "explorer.autoReveal": false,
  "files.insertFinalNewline": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Hasklig",
  "terminal.integrated.fontWeight": "600",
  "svgPreview.autoOpen": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.fontSize": 14,
  "vscode-color-picker.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg",
    "conf",
    "javascript",
    "javascriptreact",
    "markdown",
    "typescript",
    "typescriptreact"
  ],
  "colorize.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg",
    "conf",
    "javascript",
    "javascriptreact",
    "markdown",
    "typescript",
    "typescriptreact"
  ],
  "workbench.sideBar.location": "right",
  "workbench.editor.untitled.hint": "hidden",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "search.mode": "newEditor",
  "terminal.integrated.fontSize": 14,
  "redhat.telemetry.enabled": false,
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "Cryptic",
  "files.associations": {
    "*.ejs": "html"
  },
  "files.enableTrash": false,
  "emmet.includeLanguages": {
    "ejs": "html"
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
  },
  "markdown.links.openLocation": "beside",
  "window.zoomLevel": 0.25,
  "workbench.iconTheme": "eq-material-theme-icons-palenight",
  "svelte.enable-ts-plugin": true,
  "svg.preview.mode": "svg"
}
